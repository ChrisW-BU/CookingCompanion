@inherits FormInputBase
<div class="col-sm-@FieldWidth">
    <label>@Name</label><br />
    <input class="@CssClass form-control mb-3" value="@Value" autocomplete="@AutoCompleteStr"
    @onchange="UpdateParentElementStr" @onkeypress="OnInputCall" hidden="@IsHidden" disabled="@IsDisabled" />
</div>

@code {
    [Parameter]
    public EventCallback OnInputCall { get; set; }
}
